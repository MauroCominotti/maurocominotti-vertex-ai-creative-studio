  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--port",
        "8080"
      ],
      "env": {
        "FRONTEND_URL": "http://localhost:4200",
        "ENVIRONMENT": "development",
        // "GENMEDIA_BUCKET": "creative-studio-arena-genmedia",
        // "SIGNING_SA_EMAIL": "sa-genmedia-creative-studio@creative-studio-arena.iam.gserviceaccount.com",
        "GENMEDIA_BUCKET": "creative-studio-dev-cs-be-development-bucket",
        "SIGNING_SA_EMAIL": "cs-be-development-read@creative-studio-dev.iam.gserviceaccount.com",
        "IAP_AUDIENCE": "703756029283-assivi1rbjtn0qab4m01gtg4ednev0p3.apps.googleusercontent.com",
        "PROJECT_ID": "creative-studio-dev",
        "LOG_LEVEL": "INFO"
      },
      "cwd": "${workspaceFolder}/backend",
      "jinja": true
    },
    {
      "name": "Mesop: Run main.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/venv/bin/mesop",
      // Load environment variables from a .env file in the project root.
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "main.py"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
