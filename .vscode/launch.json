{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: FastAPI",
      "type": "debugpy",
      "request": "launch",
      "module": "uvicorn",
      "args": [
        "main:app",
        "--reload",
        "--port",
        "8080"
      ],
      "env": {
        "FRONTEND_URL": "http://localhost:4200",
        "ENVIRONMENT": "development",
        "GENMEDIA_BUCKET": "creative-studio-arena-genmedia",
        "SIGNING_SA_EMAIL": "sa-genmedia-creative-studio@creative-studio-arena.iam.gserviceaccount.com",
        "PROJECT_ID": "creative-studio-arena"
      },
      "cwd": "${workspaceFolder}/backend",
      "jinja": true
    },
    {
      "name": "Mesop: Run main.py",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/venv/bin/mesop",
      // Load environment variables from a .env file in the project root.
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "main.py"
      ],
      "console": "integratedTerminal",
      "justMyCode": true
    }
  ]
}
